doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="/style.css")
        title Formulario-Test
    body
        div(id='tableBody')
            table 
                tr 
                    th ID 
                    th Nombre 
                    th Precio 
                    th Foto 
                tbody(id='tbody')
                    each value in products
                        tr
                            td= value.id    
                            td= value.name
                            td= value.price
                            td
                                img(src=value.thumbnail alt='Foto no disponible') 
